---
import PageLayout from '../../layouts/page.astro'

import Grid from '../../components/grid.astro';

import { sortByDates } from '../../helpers/index.js'

const pages = Astro.fetchContent('./*.md').sort((a, b) => sortByDates(b, a));
---

<PageLayout title="About">
    <Grid items={pages} />
</PageLayout>
